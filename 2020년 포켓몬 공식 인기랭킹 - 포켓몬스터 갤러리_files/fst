document.write('<s'+'cript async="async" type="text/javascript" src="https://static.criteo.net/js/ld/publishertag.js"></s'+'cript>'); document.writeln('<div id="criteo_slot_2419"></div>'); document.writeln('<s'+'cript>'); document.writeln('    !function(){'); document.writeln('        var criteoZoneId = 1366613;'); document.writeln('        var criteoSlotId = "criteo_slot_2419";'); document.writeln('        var passback = function () {'); document.writeln('            var width = "160", height = "600";'); document.writeln('            var div = document.getElementById(criteoSlotId);'); document.writeln('            if (div) { var ifr = document.createElement(\'iframe\'); ifr.setAttribute("id", criteoSlotId+"_iframe"), ifr.setAttribute("frameborder","0"), ifr.setAttribute("allowtransparency","true"), ifr.setAttribute("hspace","0"), ifr.setAttribute("marginwidth","0"), ifr.setAttribute("marginheight","0"), ifr.setAttribute("scrolling","no"), ifr.setAttribute("vspace","0"), ifr.setAttribute("width", width), ifr.setAttribute("height", height);'); document.writeln('            div.appendChild(ifr);'); document.writeln('            var htmlcode = "<html><head></head><body><iframe src=\'https://ad.xc.netinsight.co.kr/xc/h/WxABrJ9K/x/cpd\' frameborder=\'0\' marginwidth=\'0\' marginheight=\'0\' allowtransparency=\'true\' scrolling=\'no\' width=\'160\' height=\'600\'></iframe></body></html>";'); document.writeln('            var ifrd = ifr.contentWindow.document; ifrd.open(); ifrd.write(htmlcode); ifrd.close(); }'); document.writeln('        };'); document.writeln(''); document.writeln('        var CriteoAdUnits = { "placements": [{ "slotid": criteoSlotId, "zoneid": criteoZoneId }]};'); document.writeln('        window.Criteo = window.Criteo || {}; window.Criteo.events = window.Criteo.events || [];'); document.writeln('        var processCdbBid = function() {'); document.writeln('            var bids = Criteo.GetBidsForAdUnit(criteoSlotId);'); document.writeln('            if